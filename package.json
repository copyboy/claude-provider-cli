{
  "name": "claude-provider-cli",
  "version": "0.1.0",
  "description": "Complete solution for managing Claude Code API providers - switch between GLM, MiniMax and custom providers with ease",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "claude-provider": "./bin/claude-provider"
  },
  "scripts": {
    "dev": "ts-node src/cli.ts",
    "build": "tsc && chmod +x bin/claude-provider",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "prepare": "husky install",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "claude",
    "claude-code",
    "ai",
    "api",
    "provider",
    "cli",
    "glm",
    "minimax",
    "anthropic",
    "config",
    "switch",
    "manager"
  ],
  "author": "Qingdong Zhang <qingdong.zhang> (https://github.com/copyboy)",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/copyboy/claude-provider-cli.git"
  },
  "bugs": {
    "url": "https://github.com/copyboy/claude-provider-cli/issues"
  },
  "homepage": "https://github.com/copyboy/claude-provider-cli#readme",
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^8.2.6",
    "conf": "^10.2.0",
    "axios": "^1.6.2",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "cli-table3": "^0.6.3",
    "boxen": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.11",
    "@types/inquirer": "^9.0.7",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "prettier": "^3.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}

